{
    "env": {
      "node": true
    },
    "rules": {
        "semi": [1, "always"],
        "no-unused-vars": 1,
        "comma-dangle": 0,
        "space-before-function-paren": 1,
        "indent": ["warn", 4],
        "no-multiple-empty-lines": 1,
        "handle-callback-err": [1, "^.*(e|E)rr"],
        "no-throw-literal": 0 ,
        "prefer-const": 0
    }
}